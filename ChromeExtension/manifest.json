{
  "name": "Clean Reader",
  "version": "1.0",
  "icons": {
    "88": "icons.png"
  },
  "description": "Clean Reader by ccgauche | Un clean reader qui optimise et rend plus propre votre connexion",
  "permissions": ["<all_urls>", "contextMenus"],
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["jquery.js", "cr.js"],
      "css": ["cr.css"]
    }
  ],
  "manifest_version": 2
}
